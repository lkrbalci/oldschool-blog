{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "text"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "yt_link": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "archived"
      ],
      "default": "draft"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "string"
    },
    "keywords": {
      "type": "string"
    },
    "rich_text_blocks": {
      "type": "blocks"
    }
  }
}
